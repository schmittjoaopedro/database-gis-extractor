# Retrieving lines from Postgree
setwd("/home/joao/projects/master-degree/database-gis-extractor")
require(RPostgreSQL)
drv <- dbDriver("PostgreSQL")
con <- dbConnect(drv, user = "udesc", password = "udesc", dbname = "udesc")
if(dbExistsTable(con, "linhas")) {
ds_linhas <- dbGetQuery(con, "select ST_AsText(geom) from linhas")
}
dbDisconnect(con)
dbUnloadDriver(drv)
# Plot a route
library(ggmap)
source("convert_line_string_to_data_frame.R")
route <- convertLineStringToDataFrame(1, ds_linhas[1,1])
map_jille = get_map(location = c(lon = -48.845968, lat = -26.306806), zoom = 13, maptype = 'roadmap')
map_jille_graph = ggmap(map_jille)
map_jille_graph <- map_jille_graph + geom_point(data = route, aes(x = lng, y = lat), color = "red", size = 3)
map_jille_graph
